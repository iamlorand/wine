{PAGINATION}
<div id="adminList" class="box-shadow">
	<table class="big_table" frame="box" rules="all">
		<thead>
			<tr>
				<th>Zip Code</th>
				<th>City</th>
				<th>State</th>
				<th>Platform</th>
				<th width="230px">Action</th>
			</tr>
		</thead>
		<tbody>
		<!-- BEGIN list -->
			<tr class="row{ROW}">
				<td><a href="{SITE_URL}/admin/platform/update-zip/id/{PLATFORMLOCATIONID}">{ZIPCODE}</a></td>
				<td>{CITY}</td>
				<td>{STATE}</td>
				<td><a href="{SITE_URL}/admin/platform/view/id/{ID}">{PLATFORM}</a></td>
				<td>
					<table  class="action_table">
						<tr>
							<td width="25%"><a href="{SITE_URL}/admin/platform/update-zip/id/{PLATFORMLOCATIONID}/" title="Edit/Update" class="edit_state">Edit</a></td>
							<td width="25%"><a href="{SITE_URL}/admin/platform/delete-zip/id/{PLATFORMLOCATIONID}/" title="Delete" class="delete_state">Delete</a></td>
						</tr>
					</table>
				</td>
			</tr>
		<!-- END list -->
		</tbody>
	</table>
</div>