{PAGINATION}
<div id="adminList" class="box-shadow">
	<table class="big_table" frame="box" rules="all">
		<thead>
			<tr>
				<th style="text-align: center; width: 20px;">#</th>
				<th>Investor Name</th>
				<th>CBURL</th>
				<th width="230px">Action</th>
			</tr>
		</thead>
		<tbody>
		<!-- BEGIN list -->
			<tr clas="row{ROW}">
				<td style="text-align: center;">{INVESTORID}</td>
				<td><a href="{SITE_URL}/admin/platform/update-investor/id/{INVESTORID}">{INVESTORNAME}</a></td>
				<td>{CBURL}</td>
				<td>
					<table  class="action_table">
						<tr>
							<td width="25%"><a href="{SITE_URL}/admin/platform/update-investor/id/{INVESTORID}/" title="Edit/Update" class="edit_state">Edit</a></td>
							<td width="25%"><a href="{SITE_URL}/admin/platform/delete-investor/id/{INVESTORID}/" title="Delete" class="delete_state">Delete</a></td>
						</tr>
					</table>
				</td>
			</tr>
		<!-- END list -->
		</tbody>
	</table>
</div>