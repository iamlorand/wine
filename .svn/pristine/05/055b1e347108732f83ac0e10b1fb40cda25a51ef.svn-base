{PAGINATION}
<div id="adminList" class="box-shadow">
	<table class="big_table" frame="box" rules="all">
		<thead>
			<tr>
				<th style="text-align: center; width: 20px;">#</th>
				<th>Name</th>
				<th>Parent</th>
				<th width="230px">Action</th>
			</tr>
		</thead>
		<tbody>
		<!-- BEGIN list -->
			<tr class="row{ROW}">
				<td style="text-align: center;">{ID}</td>
				<td><a href="{SITE_URL}/admin/platform-category/update/id/{ID}">{NAME}</a></td>
				<td><a href="{SITE_URL}/admin/platform-category/update/id/{ID_PARENT}">{PARENT}</a></td>
				<td>
					<table  class="action_table">
						<tr>
							<td width="25%"><a href="{SITE_URL}/admin/platform-category/update/id/{ID}/" title="Edit/Update" class="edit_state">Edit</a></td>
							<td width="25%"><a href="{SITE_URL}/admin/platform-category/delete/id/{ID}/" title="Delete" class="delete_state">Delete</a></td>
						</tr>
					</table>
				</td>
			</tr>
		<!-- END list -->
		</tbody>
	</table>
</div>