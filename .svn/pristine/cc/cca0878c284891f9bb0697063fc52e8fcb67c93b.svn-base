<div class="box-shadow" style="width: 500px">
	<ul class="form">
		<li class="clearfix">
			Click here to Download example import file:
			<a href="{SITE_URL}/upload/model/cost-import.csv" download="cost-import-example.csv" class="button">Download Example File</a>
		</li>
	</ul>
</div>
<form action="{SITE_URL}/admin/affiliate/import" method="post" enctype="multipart/form-data">
	<div class="box-shadow" style="width: 500px">
		<ul class="form">
			<li class="clearfix">
				<input type="file" name="csv" value="" class="button"/>
			</li>
			<li class="clearfix">
				<input type="submit" name="submit" value="Save" class="button"/>
			</li>
		</ul>
	</div>
</form>
